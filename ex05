nome = input("Digite seu nome: ")
n1 =  float(input("Primeira nota: "))
n2 =  float(input("Segunda nota; "))

media = (n1 + n2) / 2

print(f'A media do aluno é: {media}')
if (media == 10):
    print('Aprovado com Distinção')
elif (media >= 7):
    print('Você foi aprovado')
else:
    print'Você foi reprovado'
